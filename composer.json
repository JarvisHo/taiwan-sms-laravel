{
    "name": "jarvisho/taiwan-sms-laravel",
    "description": "Laravel wrapper package for the taiwan sms services",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jarvisho\\TaiwanSmsLaravel\\": "src/"
        }
    },
    "version": "1.1.0",
    "autoload-dev": {
        "psr-4": {
            "Jarvisho\\TaiwanSmsLaravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Falcon Ho",
            "email": "endless640c@gmail.com"
        }
    ],
    "require": {
        "infobip/infobip-api-php-client": "^3.1",
        "pestphp/pest": "^1.21"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jarvisho\\TaiwanSmsLaravel\\TaiwanSmsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.24"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
